<h1>Showing All Article</h1>
<h2>
<%= link_to "Creative new article", new_article_path%>
</h2>
<% if @articles%>
<table>
	<tr>
	<th>Title</th>
	<th>Description</th>
	</tr>
<tbody>

<% @articles.each do |article| %>
<tr>
	<td><%= article.title %></td>
	<td><%= article.description %></td>
	<td><%= link_to "Edit", edit_article_path(article) %></td>
	<td><%= link_to "Show", article_path(article) %></td>
</tr>
<% end %>
<% end %>
</tbody>
</table>
		

